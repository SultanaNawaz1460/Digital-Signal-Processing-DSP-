      "cell_type": "markdown",
      "source": [
        "# Task-3\n",
        " i) Run the above code and comment on its output.\n",
        " ii) Change the delay and observe the difference."
      ],
      "metadata": {
        "id": "nSbsvNJll3k3"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "def compute_correlation(transmitted, received):\n",
        " \"\"\"\n",
        " Computes the cross-correlation between the transmitted and received preambles.\n",
        "\n",
        " Parameters:\n",
        " transmitted (np.array): The transmitted preamble sequence.\n",
        " received (np.array): The received signal which includes the preamble.\n",
        "\n",
        " Returns:\n",
        " correlation (np.array): The cross-correlation result.\n",
        " estimated_shift (int): The estimated shift (lag) where the correlation is maximum.\n",
        " \"\"\"\n",
        " # Use 'full' mode so that we get correlation at all possible lags.\n",
        " correlation = np.correlate(received, transmitted, mode='full')\n",
        "\n",
        " # The lag corresponding to the maximum correlation indicates where the transmitted\n",
        " # preamble best aligns with the received signal.\n",
        " max_index = np.argmax(correlation)\n",
        "\n",
        " # Compute the corresponding shift (lag). For a full correlation, the zero lag is at index (len(transmitted)-1)\n",
        " estimated_shift = max_index - (len(transmitted) - 1)\n",
        "\n",
        " return correlation, estimated_shift\n",
        "if __name__ == '__main__':\n",
        " # Example transmitted preamble (using bipolar representation: 1 and -1)\n",
        " transmitted = np.array([1, -1, 1, 1, -1, 1, -1, -1])\n",
        "\n",
        " # Simulate a received preamble:\n",
        " # For this example, we simulate the received signal by shifting the transmitted preamble,\n",
        " # embedding it in zeros, and adding a little noise.\n",
        " shift_amount = 3 # For example, the preamble appears after 3 samples delay.\n",
        " preamble_length = len(transmitted)\n",
        "\n",
        " # Create a received signal: zeros before and after the preamble.\n",
        " received_clean = np.concatenate((\n",
        " np.zeros(shift_amount),\n",
        " transmitted,\n",
        " np.zeros(shift_amount)\n",
        " ))\n",
        "\n",
        " # Add some noise to simulate a real channel (adjust noise level as needed)\n",
        " noise = np.random.normal(0, 0.1, len(received_clean))\n",
        " received = received_clean + noise\n",
        "\n",
        " # Compute the cross-correlation and find the shift\n",
        " correlation, estimated_shift = compute_correlation(transmitted, received)\n",
        "\n",
        " print(\"Estimated shift (lag):\", estimated_shift)\n",
        "\n",
        " # Prepare lags for the cross-correlation plot.\n",
        " lags = np.arange(-len(transmitted) + 1, len(received))\n",
        "\n",
        " # Plotting:\n",
        " plt.figure(figsize=(16, 6))\n",
        "\n",
        " # Subplot 1: Plot the received signal and the unshifted transmitted preamble.\n",
        " plt.subplot(1, 2, 1)\n",
        " plt.plot(received, label='Received Signal', marker='o')\n",
        "\n",
        " # Plot the transmitted preamble at its original indices (starting at index 0).\n",
        " # To better compare the two, we can plot the transmitted preamble on a separate axis offset.\n",
        " transmitted_indices = np.arange(len(transmitted))\n",
        " plt.plot(transmitted_indices, transmitted, label='Transmitted Preamble (Unshifted)',\n",
        " marker='x', linestyle='--', color='red')\n",
        "\n",
        " # Mark the expected location of the transmitted preamble in the received signal.\n",
        " plt.axvline(x=estimated_shift, color='green', linestyle='--',\n",
        " label=f'Estimated Preamble Start in Received (lag={estimated_shift})')\n",
        "\n",
        " plt.title(\"Comparison of Received Signal & Unshifted Transmitted Preamble\")\n",
        " plt.xlabel(\"Sample Index\")\n",
        " plt.ylabel(\"Amplitude\")\n",
        " plt.legend()\n",
        " # Subplot 2: Cross-correlation plot with estimated lag marked.\n",
        " plt.subplot(1, 2, 2)\n",
        " plt.plot(lags, correlation, label='Cross-Correlation', marker='o')\n",
        " plt.title('Cross-Correlation')\n",
        " plt.xlabel('Lag (samples)')\n",
        " plt.ylabel('Correlation')\n",
        " plt.axvline(estimated_shift, color='red', linestyle='--',\n",
        " label=f'Estimated Lag: {estimated_shift} samples')\n",
        " plt.legend()\n",
        " plt.tight_layout()\n",
        " plt.show()"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 352
        },
        "id": "6cPNceHgj8Jg",
        "outputId": "cd64c184-03a8-47fd-a202-d66ee654585c"
      },
      "execution_count": 7,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "Estimated shift (lag): 3\n"
          ]
        },
        {
          "output_type": "display_data",
          "data": {
            "text/plain": [
              "<Figure size 1600x600 with 2 Axes>"
            ],
            "image/png": "\n"
          },
          "metadata": {}
        }
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "def compute_correlation(transmitted, received):\n",
        "    correlation = np.correlate(received, transmitted, mode='full')\n",
        "    max_index = np.argmax(correlation)\n",
        "    estimated_shift = max_index - (len(transmitted) - 1)\n",
        "    return correlation, estimated_shift\n",
        "\n",
        "if __name__ == '__main__':\n",
        "    transmitted = np.array([1, -1, 1, 1, -1, 1, -1, -1])\n",
        "    shift_amount = int(input(\"Enter the shift amount: \"))\n",
        "    preamble_length = len(transmitted)\n",
        "\n",
        "    received_clean = np.concatenate((np.zeros(shift_amount), transmitted, np.zeros(shift_amount)))\n",
        "    noise = np.random.normal(0, 0.1, len(received_clean))\n",
        "    received = received_clean + noise\n",
        "\n",
        "    # Select the portion of the received signal from shift_amount to the end of the received signal\n",
        "    selected_received = received[shift_amount:]\n",
        "\n",
        "    correlation, estimated_shift = compute_correlation(transmitted, selected_received)\n",
        "    print(\"Estimated shift (lag):\", estimated_shift)\n",
        "\n",
        "    # Update the lags array to match the length of correlation\n",
        "    lags = np.arange(-len(transmitted) + 1, len(correlation) - len(transmitted) + 1)\n",
        "\n",
        "    plt.figure(figsize=(16, 6))\n",
        "\n",
        "    plt.subplot(2, 2, 1)\n",
        "    plt.plot(received, label='Received Signal', marker='o')\n",
        "    transmitted_indices = np.arange(len(transmitted))\n",
        "    plt.plot(transmitted_indices, transmitted, label='Transmitted Preamble (Unshifted)', marker='x', linestyle='--', color='red')\n",
        "    plt.axvline(x=estimated_shift, color='green', linestyle='--', label=f'Estimated Preamble Start in Received (lag={estimated_shift})')\n",
        "    plt.title(\"Comparison of Received Signal & Unshifted Transmitted Preamble\")\n",
        "    plt.xlabel(\"Sample Index\")\n",
        "    plt.ylabel(\"Amplitude\")\n",
        "    plt.legend()\n",
        "\n",
        "    plt.subplot(2, 2, 2)\n",
        "    plt.plot(lags, correlation, label='Cross-Correlation', marker='o')\n",
        "    plt.title('Cross-Correlation')\n",
        "    plt.xlabel('Lag (samples)')\n",
        "    plt.ylabel('Correlation')\n",
